{
  "name": "@dfhernandez/sds-react-native-components",
  "version": "1.7.3",
  "description": "React native library for shareable components",
  "author": "Armando Cierra, Daniel Hernandez <fhernandez@2600hz.com> (https://github.com/shark764)",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index",
  "source": "src/index",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "bin",
    "build",
    "tools",
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "sds-react-native-components.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "android": "react-native run-android",
    "start:android": "yarn android && yarn start",
    "expo:android": "expo start --android",
    "ios": "react-native run-ios",
    "expo:ios": "expo start --ios",
    "web": "expo start --web",
    "start": "react-native start",
    "expo:start": "BROWSER=none expo start",
    "eject": "expo eject",
    "pre-commit": "lint-staged",
    "clean": "rimraf lib",
    "build": "yarn clean && bob build",
    "prepare": "bob build && husky install && yarn svg:jsx",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettify": "yarn run format && yarn run lint:fix",
    "full-check": "yarn run prettify && yarn run typescript",
    "release": "release-it",
    "svg:jsx": "svgr --template ./templates/svgr-template.js node_modules/@2600hz/sds-telicon/telicon.svg > src/components/telicon/SvgTelicon.tsx",
    "test": "jest --watchAll",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@2600hz/sds-telicon": "^1.0.7",
    "@dfhernandez/js-utilities": "^0.4.2",
    "@dfhernandez/native-theme": "^0.1.2",
    "@expo/vector-icons": "^12.0.0",
    "expo": "~42.0.1",
    "expo-asset": "~8.3.3",
    "expo-font": "~9.2.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-unimodules": "~0.14.5",
    "react-native-web": "~0.13.12",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@svgr/cli": "^5.5.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/prop-types": "^15.7.4",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.12",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "8.3.0",
    "commitlint": "^11.0.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "jest": "^26.0.1",
    "jest-expo": "^42.0.0",
    "lint-staged": "^11.1.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "pod-install": "^0.1.0",
    "prettier": "^2.3.2",
    "react-native-builder-bob": "^0.18.1",
    "react-native-svg-transformer": "^0.14.3",
    "release-it": "^14.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "~4.0.0"
  },
  "peerDependencies": {
    "@dfhernandez/js-utilities": "*",
    "@dfhernandez/native-theme": "*",
    "react": "*",
    "react-native": "*",
    "react-native-svg": "*",
    "styled-components": "*"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "keywords": [
    "android",
    "components",
    "design",
    "ios",
    "mobile",
    "react",
    "react-native",
    "sds",
    "svg",
    "ui"
  ],
  "license": "MIT",
  "homepage": "https://github.com/2600hz/sds-react-native-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2600hz/sds-react-native-components.git"
  },
  "bugs": {
    "url": "https://github.com/2600hz/sds-react-native-components/issues"
  }
}
